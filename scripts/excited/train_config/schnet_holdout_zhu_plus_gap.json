{
    "model_params": {
        "model_type": "SchNetDiabat",
        "cutoff": 5.0,
        "diabat_keys": [
            [
                "d0",
                "lam"
            ],
            [
                "lam",
                "d1"
            ]
        ],
        "dropout_rate": 0.0,
        "grad_keys": [
            "energy_0_grad",
            "energy_1_grad"
        ],
        "n_atom_basis": 256,
        "n_convolutions": 4,
        "n_filters": 256,
        "n_gaussians": 32,
        "output_keys": [
            "energy_0",
            "energy_1"
        ],
        "readoutdict": {
            "d0": [
                {
                    "name": "linear",
                    "param": {
                        "in_features": 256,
                        "out_features": 128
                    }
                },
                {
                    "name": "shifted_softplus",
                    "param": {}
                },
                {
                    "name": "linear",
                    "param": {
                        "in_features": 128,
                        "out_features": 1
                    }
                }
            ],
            "d1": [
                {
                    "name": "linear",
                    "param": {
                        "in_features": 256,
                        "out_features": 128
                    }
                },
                {
                    "name": "shifted_softplus",
                    "param": {}
                },
                {
                    "name": "linear",
                    "param": {
                        "in_features": 128,
                        "out_features": 1
                    }
                }
            ],
            "lam": [
                {
                    "name": "linear",
                    "param": {
                        "in_features": 256,
                        "out_features": 128
                    }
                },
                {
                    "name": "shifted_softplus",
                    "param": {}
                },
                {
                    "name": "linear",
                    "param": {
                        "in_features": 128,
                        "out_features": 1
                    }
                }
            ]
        },
        "trainable_gauss": true
    },
    "train_params": {
        "sampler": {
            "name": "BalancedFFSampler",
            "apply_to": [
                "train",
                "val",
                "test"
            ],
            "sampler_paths": {
                "train": "train_sample_dict.json",
                "val": "val_sample_dict.json",
                "test": "test_sample_dict.json"
            }
        },
        "batch_size": 64,
        "mini_batches": 1,
        "checkpoints_to_keep": 3,
        "del_grad_interval": 100,
        "log_every_n_epochs": 1,

        "multi_loss_dict": {"mse": [{"coef": 0.2, "params": {"key": "energy_0"}},
                                    {"coef": 0.1, "params": {"key": "energy_1"}},
                                    {"coef": 1.0, "params": {"key": "energy_0_grad"}},
                                    {"coef": 1.0, "params": {"key": "energy_1_grad"}},
                                    {"coef": 1.0, "params": {"key": "energy_1_energy_0_delta"}}],

                           "zhu": [{"coef": 10,
                                   "params": {"loss_type": "cross_entropy",
                                   "expected_gap": 0.00551,
                                   "lower_energy": "energy_0",
                                   "upper_energy": "energy_1"}
                                   }],

                           "zhu_grad": [{"coef": 100,
                                         "params": {"loss_type": "mse",
                                                    "expected_gap": 0.00551,
                                                    "lower_energy": "energy_0",
                                                    "upper_energy": "energy_1"}}
                                       ]},
        "lr": 1e-5,
        "lr_decay": 0.5,
        "lr_min": 5e-06,
        "lr_patience": 15,
        "max_epochs": 1000,
        "metrics": [
            "MeanAbsoluteError"
        ],

        "weightpath": "/nfs/rafagblab001/saxelrod/dsets_switches",
        "model_name": "schnet_holdout_plus_gap",
        "mol_loss_norm": true,
        "seed": 0,
        "torch_par": false,
        "use_slurm": true,
        "needs_nbrs": true,
        "needs_angles": false,
        "cutoff": 5.0,
        "reset_trainer": true
    }
}
